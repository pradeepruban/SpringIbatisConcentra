/**
 *This method is used to intiate an ajax call to get User Application Queue link Details list
 * @return success function/ error function
*/
function fnGetUsrAppQLnkDetails()
{
	  var getGetUsrAppQLnkDetailsJsonObj = {};
	  var strJSON = JSON.stringify(getGetUsrAppQLnkDetailsJsonObj);
	  fnCommonDoAjaxForGet("<%=request.getContextPath()%>/pages/usrAppQLinkDetails", strJSON,  "fnSucAfterGetUsrAppQLnkDetails", "fnErrAfterGetUsrAppQLnkDetails");
	
}
/**This method is called on success of fnCommonDoAjaxForGet util method with action get User Application Queue link Details list
* @param Queue details
*/
function fnSucAfterGetUsrAppQLnkDetails(UsrAppQLinkDtls)
{
	fnLoadDataTable(varUsrAppQLinkDataTable, UsrAppQLinkDtls);
	varUsrAppQLinkDataTable.find('tbody tr:first').addClass('row_selected');
}
/**This method is called on failure of fnCommonDoAjaxForGet util method with action get User Application Queue link Details list
*/
function fnErrAfterGetUsrAppQLnkDetails()
{
	alert("User Application Queue link Details error");
} 
/**This method is called on clicking of row in User Application Queue link Details data table
*/
$("#usrAppQLinkDataTableId tbody").on('click', 'tr', function() {
	
	$("#usrAppQLinkDataTableId tbody tr").removeClass('row_selected');	
    $(this).addClass('row_selected');
	
});
function fnItOpsUsrAppQueueLinkAdd(){
	$('#usrAppQLinkUsrNameSelectId option').remove();
	$('#usrAppQLinkApplnNameSelectId option').remove();
	$('#usrAppQLinkQueNameSelectId option').remove();
	fnItOpsGetUsrNmsList();
}
function fnItOpsGetUsrNmsList(){
	var varGetUsrDetailsJsonObj = {};
	var strJSON = JSON.stringify(varGetUsrDetailsJsonObj);
	fnCommonDoAjaxForGet("<%=request.getContextPath()%>/pages/usrdetailslist", strJSON,  "fnUsrAppQueLnkSuccGetUsersNmsList", "fnUsrAppQueLnkErrGetUsersNmsList");	
}
function fnUsrAppQueLnkSuccGetUsersNmsList(varUserDtlsToView){
	//alert("success"+varUserDtlsToView);
	var varUAQLUsrNamesListOptns = '';
	for ( var i = 0; i < varUserDtlsToView.aaData.length; i++) {
		 varUAQLUsrNamesListOptns += '<option value="' +varUserDtlsToView.aaData[i].usrId+ '">'+ varUserDtlsToView.aaData[i].usrShortName + '</option>';
	} 
	$('#usrAppQLinkUsrNameSelectId').append(varUAQLUsrNamesListOptns);
	fnItOpsUsrAppQueLnkGetAppNmsList();
}
function fnUsrAppQueLnkErrGetUsersNmsList(){
	alert("failure");
}
function fnItOpsUsrAppQueLnkGetAppNmsList()
{
	  var getAppNamesListJsonObj = {};
	  var strJSON = JSON.stringify(getAppNamesListJsonObj);
	  fnCommonDoAjaxForGet("<%=request.getContextPath()%>/pages/getApplicationDtlsToView", strJSON,  "fnUsrAppQueLnkSuccGetAppNmsList", "fnUsrAppQueLnkErrGetAppNmsList");
	
}
function fnUsrAppQueLnkSuccGetAppNmsList(varApplicationDtlsToView){
	//alert("success"+varApplicationDtlsToView);
	var varUAQLApplnNamesListOptns = '';
	for ( var i = 0; i < varApplicationDtlsToView.aaData.length; i++) {
		varUAQLApplnNamesListOptns += '<option value="' +varApplicationDtlsToView.aaData[i].applnId+ '">'+ varApplicationDtlsToView.aaData[i].applnName + '</option>';
	} 
	$('#usrAppQLinkApplnNameSelectId').append(varUAQLApplnNamesListOptns);
	fnItOpsUsrAppQueLnkQueNmsList();
}
function fnUsrAppQueLnkErrGetAppNmsList(){
	alert("failure");
}
function fnItOpsUsrAppQueLnkQueNmsList()
{
	  //alert("inside fnItOpsUsrAppQueLnkQueNmsList");
	  var getQueueNamesListJsonObj = {};
	  var strJSON = JSON.stringify(getQueueNamesListJsonObj);
	  fnCommonDoAjaxForGet("<%=request.getContextPath()%>/pages/getQueueDtls", strJSON,  "fnUsrAppQueLnkSuccGetQueNmsList", "fnUsrAppQueLnkErrGetQueNmsList");
}
function fnUsrAppQueLnkSuccGetQueNmsList(varQueueDtlsToView){
	//alert("success"+varQueueDtlsToView);
	varvarUAQLQueNamesListOptions = '';
	for ( var i = 0; i < varQueueDtlsToView.aaData.length; i++) {
		varvarUAQLQueNamesListOptions += '<option value="' +varQueueDtlsToView.aaData[i].queId + '">'+ varQueueDtlsToView.aaData[i].queName + '</option>';
	}
	$('#usrAppQLinkQueNameSelectId').append(varvarUAQLQueNamesListOptions);
	$('#itOpsUsrAppQueueLnkAddPopUp').modal('show');
}
function fnUsrAppQueLnkErrGetQueNmsList(){
	alert("failure");
}
function fnItOpsValUsrApplnQueLinkDtls(){
	//alert("inside fnItOpsValUsrApplnQueLinkDtls");
	var varValUsrAppQueLnkDtlsJsonObj = {};
	varValUsrAppQueLnkDtlsJsonObj['usrAppQLinkUsrId'] =document.getElementById("usrAppQLinkUsrNameSelectId").value;
	varValUsrAppQueLnkDtlsJsonObj['usrAppQLinkApplicationId'] =document.getElementById("usrAppQLinkApplnNameSelectId").value;
	varValUsrAppQueLnkDtlsJsonObj['usrAppQLinkQueueId'] =document.getElementById("usrAppQLinkQueNameSelectId").value;
	var strJSON = JSON.stringify(varValUsrAppQueLnkDtlsJsonObj);
	//alert('strJSON is:'+strJSON+':');	
	fnCommonDoAjaxForGet("<%=request.getContextPath()%>/pages/valUsrApplnQueLinkDtls", strJSON,  "fnItOpsSuccValUsrApplnQueLinkDtls", "fnItOpsErrValUsrApplnQueLinkDtls"); 
}
function fnItOpsSuccValUsrApplnQueLinkDtls(varCountUsrApplnQueLink){
	//alert("success"+varCountUsrApplnQueLink);
	if(varCountUsrApplnQueLink==0){
		fnItOpsAddUsrApplnQueLinkDtls();
	}
	else{
		alert("User Application Queue Link already exists");
	} 
}
function fnItOpsErrValUsrApplnQueLinkDtls(){
	alert("failure");
}
function fnItOpsAddUsrApplnQueLinkDtls(){
	var varAddUsrApplnQueLnkJsonObj = {};
	
	varAddUsrApplnQueLnkJsonObj['usrAppQLinkUsrId'] =document.getElementById("usrAppQLinkUsrNameSelectId").value;
	varAddUsrApplnQueLnkJsonObj['usrAppQLinkApplicationId'] =document.getElementById("usrAppQLinkApplnNameSelectId").value;
	varAddUsrApplnQueLnkJsonObj['usrAppQLinkQueueId'] =document.getElementById("usrAppQLinkQueNameSelectId").value;
    var UAQLAddToViewChckBox=document.getElementById("usrAppQLinkAddToViewChckBox").checked;
    //alert("UAQLAddToViewChckBox"+UAQLAddToViewChckBox);
	if(UAQLAddToViewChckBox==true){
		varAddUsrApplnQueLnkJsonObj['usrAppQLinkToViewFlag'] = 1;
	}
	else{
		varAddUsrApplnQueLnkJsonObj['usrAppQLinkToViewFlag'] = 0;
	}
	
	var UAQLAddToExeChckBox=document.getElementById("usrAppQLinkAddToExeChckBox").checked;
    //alert("UAQLAddToExeChckBox"+UAQLAddToExeChckBox);
	if(UAQLAddToExeChckBox==true){
		varAddUsrApplnQueLnkJsonObj['usrAppQLinkToExecuteFlag'] = 1;
	}
	else{
		varAddUsrApplnQueLnkJsonObj['usrAppQLinkToExecuteFlag'] = 0;
	}
	//alert("values"+varAddUsrApplnQueLnkJsonObj['usrAppQLinkUsrId']+varAddUsrApplnQueLnkJsonObj['usrAppQLinkApplicationId']+varAddUsrApplnQueLnkJsonObj['usrAppQLinkQueueId']+varAddUsrApplnQueLnkJsonObj['usrAppQLinkToViewFlag']+varAddUsrApplnQueLnkJsonObj['usrAppQLinkToExecuteFlag']);
	var strJSON = JSON.stringify(varAddUsrApplnQueLnkJsonObj);
	//alert('strJSON is:'+strJSON+':');	
	if (confirm("Are you sure you want to Add details") == true) {
	fnCommonDoAjaxForSave("<%=request.getContextPath()%>/pages/addUsrApplnQueLinkDtls", strJSON,  "fnSuccAddUsrApplnQueLinkDtls", "fnErrAddUsrApplnQueLinkDtls"); 
	}
	else{
		$('#itOpsUsrAppQueueLnkAddPopUp').modal('hide');
	}
}
function fnSuccAddUsrApplnQueLinkDtls(){
	//alert("success");
	$('#itOpsUsrAppQueueLnkAddPopUp').modal('hide');
	fnGetUsrAppQLnkDetails();
	
}
function fnErrAddUsrApplnQueLinkDtls(){
	alert("failure");
}
function fnPopulateUsrApplnQLnkDtlsInPopUp(varUsrAppQLnkIdCon){
	//alert("inside fnPopulateUsrApplnQLnkDtlsInPopUp"+varUsrAppQLnkIdCon);
	$("#usrAppQLinkIdEditPopUp").val(varUsrAppQLnkIdCon);
	//alert("usrAppQLinkIdEditPopUp:  " +getValuesFromInputField("usrAppQLinkIdEditPopUp"));
	
	var allUsrApplnQLnkData = $('#usrAppQLinkDataTableId').DataTable().rows().data();
	for(var i=0;i<allUsrApplnQLnkData.length;i++)
	{
	var tempUsrApplnQLnkData = allUsrApplnQLnkData [i];
	//alert("inside fnPopulateUsrApplnQLnkDtlsInPopUp fn., tempUsrApplnQLnkData is:"+tempUsrApplnQLnkData +':tempUsrApplnQLnkData [usrAppQLinkId]:'+tempUsrApplnQLnkData ['usrAppQLinkId']);
	var tempUsrApplnQLnkId = tempUsrApplnQLnkData ['usrAppQLinkId'];
	if(tempUsrApplnQLnkId == varUsrAppQLnkIdCon)
		{
		    //alert(" in if tempUsrApplnQLnkId"+tempUsrApplnQLnkId)
			$("#usrAppQLinkUsrNameEditPopUp").val(tempUsrApplnQLnkData ['strUsrAppQLinkUsrName']);
			$("#usrAppQLinkAppNameEditPopUp").val(tempUsrApplnQLnkData ['strUsrAppQLinkApplicationName']);
			$("#usrAppQLinkQueueNameEditPopUp").val(tempUsrApplnQLnkData ['strUsrAppQLinkQueueName']);
			
			if(tempUsrApplnQLnkData ['usrAppQLinkToViewFlag']==1){
				document.getElementById("usrAppQLinkAddToViewEditChckBox").checked = true;
			}
			else{
			document.getElementById("usrAppQLinkAddToViewEditChckBox").checked = false;
			}
			
			if(tempUsrApplnQLnkData ['usrAppQLinkToExecuteFlag']==1){
				document.getElementById("usrAppQLinkAddToExeEditChckBox").checked = true;
			}
			else{
			document.getElementById("usrAppQLinkAddToExeEditChckBox").checked = false;
			}
			
			if(tempUsrApplnQLnkData ['usrAppQLinkDeleteFlag']==1){
				document.getElementById("usrAppQLinkEditDltChckBox").checked = true;
			}
			else{
			document.getElementById("usrAppQLinkEditDltChckBox").checked = false;
			}
			
			$('#itOpsUsrAppQueueLnkEditPopUp').modal('show');
			break;
		}
	
	} 
}
function fnUpdateUsrApplnQueLinkDtls(){
	var varUpdtUsrAppQLnkJsonObj = {};
	
	varUpdtUsrAppQLnkJsonObj['usrAppQLinkId'] = getValuesFromInputField("usrAppQLinkIdEditPopUp");
    var varUpdtUsrAppQLnkToView=document.getElementById("usrAppQLinkAddToViewEditChckBox").checked;
	if(varUpdtUsrAppQLnkToView==true){
		varUpdtUsrAppQLnkJsonObj['usrAppQLinkToViewFlag'] = 1;
	}
	else{
		varUpdtUsrAppQLnkJsonObj['usrAppQLinkToViewFlag'] = 0;
	}
	
    var varUpdtUsrAppQLnkToExec=document.getElementById("usrAppQLinkAddToExeEditChckBox").checked;
	if(varUpdtUsrAppQLnkToExec==true){
		varUpdtUsrAppQLnkJsonObj['usrAppQLinkToExecuteFlag'] = 1;
	}
	else{
		varUpdtUsrAppQLnkJsonObj['usrAppQLinkToExecuteFlag'] = 0;
	}
		
	var varUpdtUsrAppQLnkDelete=document.getElementById("usrAppQLinkEditDltChckBox").checked;
	if(varUpdtUsrAppQLnkDelete==true){
		varUpdtUsrAppQLnkJsonObj['usrAppQLinkDeleteFlag'] = 1;
	}
	else{
		varUpdtUsrAppQLnkJsonObj['usrAppQLinkDeleteFlag'] = 0;
	}
	
	var strJSON = JSON.stringify(varUpdtUsrAppQLnkJsonObj);
	//alert('strJSON is:'+strJSON+':');	
	if (confirm("Are you sure you want to edit details") == true) {
	fnCommonDoAjaxForSave("<%=request.getContextPath()%>/pages/updateUsrApplnQueLinkDtls", strJSON,  "fnSuccUpdateUsrApplnQueLinkDtls", "fnErrUpdateUsrApplnQueLinkDtls"); 
	}
	else{
		$('#itOpsUsrAppQueueLnkEditPopUp').modal('hide');
	}
}
function fnSuccUpdateUsrApplnQueLinkDtls(){
	//alert("success");
	$('#itOpsUsrAppQueueLnkEditPopUp').modal('hide');
	fnGetUsrAppQLnkDetails();
	
}
function fnErrUpdateUsrApplnQueLinkDtls(){
	alert("failure");
}
