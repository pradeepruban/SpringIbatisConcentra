/**
 *This function is used to get data table properties of user application queue link data table
 */
var varUsrAppQLinkDataTable;
function fnInitializeUsrAppQLinkkDataTable(){
	//alert("Inside Dtable")
	varUsrAppQLinkDataTable = $('#usrAppQLinkDataTableId').dataTable({
