																	if (data == '0') {
																		 return 'No';
															        }
																	if (data == '1') {
																		 return 'Yes';
															        }
																}
															},{
																"data" : "usrAppQLinkToExecuteFlag",
																"mRender": function (data, type, row) {
																	if (data == '0') {
																		 return 'No';
															        }
																	if (data == '1') {
																		 return 'Yes';
															        }
																}
															},{
																"data" : "usrAppQLinkDeleteFlag",
																"mRender": function (data, type, row) {
																	if( (typeof(data) !="undefined")  && (data != 'undefined') && (data != null) ){
