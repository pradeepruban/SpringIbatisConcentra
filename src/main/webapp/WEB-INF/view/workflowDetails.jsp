    if(varWrkflwNamePopUp == null || varWrkflwNamePopUp == "") {
    	varItOpsWrkflwDtlsValidationErrMsg = "Workflow Name";
    	setInvalidInputFieldBorderRed('wrkflwNamePopUp');
    }
	if(varWrkflwURLPopUp == null || varWrkflwURLPopUp == ""){
			if(varItOpsWrkflwDtlsValidationErrMsg.length > 0)
			{
