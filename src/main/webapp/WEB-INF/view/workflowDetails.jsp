			}
			varItOpsWrkflwDtlsValidationErrMsg += "Workflow URL";
			setInvalidInputFieldBorderRed('wrkflwURLPopUp');
	}
	if(varWrkflwDeploymentIdPopUp == null || varWrkflwDeploymentIdPopUp == ""){
		if(varItOpsWrkflwDtlsValidationErrMsg.length > 0)
		{
			varItOpsWrkflwDtlsValidationErrMsg += ", ";
		}
		varItOpsWrkflwDtlsValidationErrMsg += "Workflow Deployment Id";
		setInvalidInputFieldBorderRed('wrkflwdeplymntIdPopUp');
	}
	if(varWrkflwDefIdPopUp == null || varWrkflwDefIdPopUp == ""){
		if(varItOpsWrkflwDtlsValidationErrMsg.length > 0)
		{
			varItOpsWrkflwDtlsValidationErrMsg += ", ";
		}
		varItOpsWrkflwDtlsValidationErrMsg += "Workflow Definition Id";
		setInvalidInputFieldBorderRed('wrkflwDefIdPopUp');
	}
	if(varWrkflwUsrnamePopUp == null || varWrkflwUsrnamePopUp == ""){
		if(varItOpsWrkflwDtlsValidationErrMsg.length > 0)
		{
			varItOpsWrkflwDtlsValidationErrMsg += ", ";
		}
		varItOpsWrkflwDtlsValidationErrMsg += "Workflow Username";
		setInvalidInputFieldBorderRed('wrkflwUsrnamePopUp');
	}
	if(varWrkflwPwdPopUp == null || varWrkflwPwdPopUp == ""){
		if(varItOpsWrkflwDtlsValidationErrMsg.length > 0)
		{
			varItOpsWrkflwDtlsValidationErrMsg += ", ";
		}
		varItOpsWrkflwDtlsValidationErrMsg += "Workflow Password";
		setInvalidInputFieldBorderRed('wrkflwPwdPopUp');
	}
	
	if(varItOpsWrkflwDtlsValidationErrMsg.length > 0)
	{
		varItOpsWrkflwDtlsValidationErrMsg += " are mandatory fields. ";
		//alert("varItOpsWrkflwDtlsValidationErrMsg"+varItOpsWrkflwDtlsValidationErrMsg);
	}
	if(varItOpsWrkflwDtlsValidationErrMsg == null || varItOpsWrkflwDtlsValidationErrMsg == ''){
		fnItOpsValWrkflwDtls();
	}else{
		//alert(varItOpsWrkflwDtlsValidationErrMsg+" Fields are Required:");
		fnShowItOpsValidationDialog(varItOpsWrkflwDtlsValidationErrMsg);
	}	
}

