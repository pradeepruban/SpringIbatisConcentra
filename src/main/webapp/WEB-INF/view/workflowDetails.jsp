/**This method is calledto load workflow param details data table with corresponding param values
 * @param workflow param values
*/
function fnSetWrkflwParamDtlsTable(varData)
{
	var wrkflwParamList=varData['workflowParamLnkList'];
	fnLoadDataTableFromList(itOpsVarWrkflwParamDataTable, wrkflwParamList);
	itOpsVarWrkflwParamDataTable.find('tbody tr:first').addClass('row_selected');
	fnClearWrkflwParamDtls();
	if((typeof(wrkflwParamList)!="undefined") && (wrkflwParamList != 'undefined') && (wrkflwParamList != null) && (wrkflwParamList.length > 0)){
		fnSetWrkflwParamDtls(wrkflwParamList[0]);
	}
	
	
	$("#wrkflwIdParamPopUp").val(varData['wrkflwId']);
}
