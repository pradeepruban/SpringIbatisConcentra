function fnPopulateWrkflwParamDtlsInPopup(varItOpsWrkflwId,varItOpsWrkflwParamLnkId){
	//alert("inside fnPopulateWrkflwParamDtlsInPopup");
	var allWrkflwParamData = $('#itOpsWrkflwParam').DataTable().rows().data();
	for(var i=0;i<allWrkflwParamData.length;i++)
	{
		var tempWrkflwParamData = allWrkflwParamData [i];
		//alert("inside fnPopulateWrkflwParamDtlsInPopup fn., tempWrkflwParamData is:"+tempWrkflwParamData +':tempWrkflwParamData [wrkflwParamLnkId]:'+tempWrkflwParamData ['wrkflwParamLnkId']+':');
		var tempWrkflwParamLnkId = tempWrkflwParamData ['wrkflwParamLnkId'];
		if(tempWrkflwParamLnkId == varItOpsWrkflwParamLnkId)
		{
			
